{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 1,
        "no-underscore-dangle": 0,
        "linebreak-style": 0
    }
}
